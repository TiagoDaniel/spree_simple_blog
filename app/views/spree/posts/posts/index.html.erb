<h1>Blog</h1>
<div id="posts-page-container">
	<%@posts.each do |p|%>
		<div class="post-container">
			<div class="post">
				<h4><%= p.title%></h4>
				<p><%=p.body.html_safe%></p>
			</div>
			<span class="post-time">
				<p>Created at <%=localize p.created_at, format: :long %></p>
			</span>
		</div>
	<%end%>
</div>